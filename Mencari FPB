def hitung_FPB(x,y):
    if x>y:
       smaller=y
    else:
       smaller=x
    for i in range (1, smaller+1):
       if ((x%i==0) and (y%i==0)):
          FPB=i
    return FPB
bilangan_1=int(input("Masukkan bilangan 1:"))
bilangan_2=int(input("Masukkan bilangan 2:"))
print("FPB dari",bilangan_1 "dan",bilangan_2, "=", hitung_FPB(bilangan_1, bilangan_2))
